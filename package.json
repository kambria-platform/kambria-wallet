{
  "name": "@kambria/kambria-wallet",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "start": "HTTPS=true REACT_APP_ENV=development react-scripts start",
    "build": "npm run clean && babel wallet -d dist && babel exports.js -o index.js",
    "clean": "rm -rf dist && rm -rf index.js",
    "test": "npm run pre-test && npm start",
    "pre-test": "npm run build && npm run fake-module",
    "fake-module": "rm -rf ./node_modules/@kambria && mkdir -p ./node_modules/@kambria/kambria-wallet && cp -R ./dist ./node_modules/@kambria/kambria-wallet/dist && cp ./index.js ./node_modules/@kambria/kambria-wallet/index.js"
  },
  "author": "tphanson <tphanson@kambria.io>",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@ledgerhq/hw-app-eth": "^4.32.0",
    "@ledgerhq/hw-transport-u2f": "^4.32.0",
    "async": "^2.6.1",
    "bip39": "^2.5.0",
    "crypto-js": "^3.1.9-1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.0.0",
    "ethereumjs-wallet": "^0.6.2",
    "hdkey": "^1.1.0",
    "react-modal": "^3.8.1",
    "web3": "^0.20.6",
    "web3-provider-engine": "^14.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "connected-react-router": "^6.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
