{
  "name": "@kambria/kambria-wallet",
  "version": "2.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "start": "",
    "build": "npm run clean && npm run build-js && npm run build-css",
    "clean": "rm -rf temp && rm -rf index.js",
    "test": "npm run pre-test && HTTPS=true REACT_APP_ENV=development react-scripts start",
    "pre-test": "npm run build && npm run fake-module",
    "fake-module": "rm -rf ./node_modules/@kambria && mkdir -p ./node_modules/@kambria/kambria-wallet && cp -R ./temp ./node_modules/@kambria/kambria-wallet/temp && cp ./index.js ./node_modules/@kambria/kambria-wallet/index.js",
    "build-js": "babel exports.js -o index.js && babel wallet -d temp",
    "build-css": "npm run scss && rsync -av --prune-empty-dirs --exclude='*.scss' ./wallet/skin/static/ ./temp/skin/static/",
    "scss": "sass ./wallet/skin/static/styles/index.scss ./wallet/skin/static/styles/index.css"
  },
  "author": "tphanson <tphanson@kambria.io>",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "@ledgerhq/hw-app-eth": "^4.41.1",
    "@ledgerhq/hw-transport-u2f": "^4.41.1",
    "async": "^2.6.1",
    "bip39": "^2.5.0",
    "connected-react-router": "^6.3.0",
    "crypto-js": "^3.1.9-1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.0.0",
    "ethereumjs-wallet": "^0.6.2",
    "hdkey": "^1.1.0",
    "keythereum": "^1.0.4",
    "prop-types": "^15.7.1",
    "react": "^16.6.3",
    "react-bootstrap4-modal": "^1.7.0",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "web3": "^0.20.6",
    "web3-provider-engine": "^14.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
